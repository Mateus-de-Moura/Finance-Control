@model Finance.Control.Domain.Entities.Category

@{
    ViewData["Title"] = "Edição de Categorias";
}

<form asp-action="Edit" asp-controller="Category" method="post">

    <div class="col-12 mb-2 mb-xl-3 d-flex">
        <div class="col-auto">
            <h5>Cadastro de Categorias</h5>
        </div>

        <div class="col-auto ms-auto text-end mt-1">
            <a class="btn btn-outline-secondary" asp-action="Index">
                Voltar
            </a>
        </div>
    </div>

    <partial name="Partials/_Form" model="@Model" />
</form>